  "scripts": {
    "build": "env SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider --max_old_space_size=4096 node scripts/build.js",
    "build:css": "sass --no-source-map --style=compressed -q --load-path=./node_modules src/index.scss:src/css/index.css src/App.scss:src/css/App.css",
    "fix": "concurrently -n prettier,eslint,css -c green,yellow,cyan \"npm run fix:prettier\" \"npm run fix:eslint\" \"npm run lint:css\"",
    "fix:eslint": "eslint --config .eslintrc.js --ignore-path .eslintignore --fix \"**/*.{ts,tsx,js,jsx}\"",
    "fix:prettier": "pprettier --concurrency 50 --ignore-path .prettierignore --write \"**/*.{ts,tsx,json,js,jsx,md}\"",
    "fix:css": "stylelint --config .stylelintrc --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint": "concurrently -n prettier,eslint,css -c green,yellow,cyan \"npm run lint:prettier\" \"npm run lint:eslint\" \"npm run lint:css\"",
    "lint:eslint": "eslint --config .eslintrc.js --ignore-path ../.eslintignore \"**/*.{ts,tsx,js,jsx}\"",
    "lint:prettier": "pprettier --concurrency 50 --check --ignore-path ../.prettierignore \"**/*.{ts,tsx,json,js,jsx,md}\"",
    "lint:css": "stylelint --config .stylelintrc './src/**/*.{js,jsx,ts,tsx}'",
    "prod": "serve -s build -p 3000",
    "start": "BROWSER=browser-open.js npm-run-all -p build:css watch:css start-js",
    "start-js": "env SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider node scripts/start.js",
    "start:prod": "npm-run-all -s prod",
    "start:staging": "npm-run-all -p build:css watch:css start-js",
    "test": "env SKIP_PREFLIGHT_CHECK=true node scripts/test.js --transformIgnorePatterns \"node_modules/(?!axios)/\" --env=jsdom --testPathIgnorePatterns=src/superrare-ui",
    "watch:css": "sass --no-source-map --style=compressed -q --load-path=./node_modules -w src/index.scss:src/css/index.css src/App.scss:src/css/App.css"
  },
